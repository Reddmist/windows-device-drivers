; --------------------------------------------
; USB CDC ACM Setup File
; --------------------------------------------

[Version] 
Signature="$Windows NT$" 
Class=Ports
ClassGuid={4D36E978-E325-11CE-BFC1-08002BE10318} 
Provider=%PROVIDER% 
LayoutFile=layout.inf
DriverVer=04/24/2016,6.0.0.0
CatalogFile=electron.cat

[Manufacturer] 
%PROVIDER%=DeviceList, NTx86, NTia64, NTamd64, NTARM

[DeviceList.NTx86] 
%DEVICE_DESCRIPTION1%=DriverInstall, USB\VID_2B04&PID_C00A&MI_00
%DEVICE_DESCRIPTION2%=DriverInstall, USB\VID_2B04&PID_C00A&MI_02

[DeviceList.NTia64] 
%DEVICE_DESCRIPTION1%=DriverInstall, USB\VID_2B04&PID_C00A&MI_00
%DEVICE_DESCRIPTION2%=DriverInstall, USB\VID_2B04&PID_C00A&MI_02

[DeviceList.NTamd64] 
%DEVICE_DESCRIPTION1%=DriverInstall, USB\VID_2B04&PID_C00A&MI_00
%DEVICE_DESCRIPTION2%=DriverInstall, USB\VID_2B04&PID_C00A&MI_02

[DeviceList.NTARM] 
%DEVICE_DESCRIPTION1%=DriverInstall, USB\VID_2B04&PID_C00A&MI_00
%DEVICE_DESCRIPTION2%=DriverInstall, USB\VID_2B04&PID_C00A&MI_02

[DriverCopyFiles]
usbser.sys,,,0x20
;serenum.sys,,,0x20
lowcdc.sys

;------------------------------------------------------------------------------
;  Installation Configuration
;------------------------------------------------------------------------------

[DriverInstall]
include=mdmcpq.inf 
CopyFiles=DriverCopyFiles
AddReg=DriverInstall.AddReg 

[DriverInstall.HW] 
;AddReg=DriverInstall.HW.AddReg 
AddReg=DriverLower.HW

[DriverInstall.AddReg]
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"

[DriverInstall.HW.AddReg]
HKR,,NTMPDriver,,*ntkern
HKR,,NTMPDriver,,usbser.sys
HKR,,PortSubClass,1,01
;HKR,,"UpperFilters",0x00010000,"serenum" 

[DriverInstall.Services] 
AddService=usbser, 0x00000002, UsbserService
AddService=lowcdc, 0x00000000, DriverLower.AddService
;AddService=Serenum,,SerenumService

[DriverLower.HW]
HKR,,LowerFilters, 0x00010000, "lowcdc"

;------------------------------------------------------------------------------
;  Usbser Service
;------------------------------------------------------------------------------

[UsbserService] 
DisplayName=%USBSER.DESC% 
ServiceType=1
StartType=3
ErrorControl=1
ServiceBinary=%12%\usbser.sys 
LoadOrderGroup = PNP Filter

[DriverLower.AddService]
DisplayName=%LOWCDC.DESC%
ServiceType=1
StartType=3
ErrorControl=1
ServiceBinary=%12%\lowcdc.sys

;------------------------------------------------------------------------------
;  Serenum Service
;------------------------------------------------------------------------------

;[SerenumService] 
;DisplayName    = %SERENUM.DESC%
;ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
;StartType      = 3               ; SERVICE_DEMAND_START
;ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
;ServiceBinary  = %12%\serenum.sys
;LoadOrderGroup = PNP Filter

;------------------------------------------------------------------------------
;  String Definitions
;------------------------------------------------------------------------------

[Strings] 
PROVIDER="Particle"
DEVICE_DESCRIPTION1 ="Electron Serial"
DEVICE_DESCRIPTION2 ="Electron USBSerial1"
USBSER.DESC = "USB Serial Driver"
LOWCDC.DESC = "lowcdc driver"
;SERENUM.DESC = "Serenum Filter Driver"
